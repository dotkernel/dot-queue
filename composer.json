{
    "name": "dotkernel/dot-queue",
    "type": "library",
    "description": "DotKernel queue component",
    "license": "MIT",
    "authors": [
        {
            "name": "n3vrax",
            "email": "tibi@apidemia.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "laminas/laminas-servicemanager": "^3.3",
        "dotkernel/dot-console": "^2.0",
        "phplucidframe/console-table": "^1.2",
        "ramsey/uuid": "^3.7",
        "laminas/laminas-validator": "^2.10",
        "laminas/laminas-filter": "^2.7",
        "psr/log": "^1.0",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3",
        "laminas/laminas-db": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Queue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Queue\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-develop": "2.1-dev"
        }
    }
